<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ingemark.cordova.pushdemo" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hello Cordova</name>
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <name>PushDemo</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="com.urbanairship.production_app_key" value="Your production app key" />
    <preference name="com.urbanairship.production_app_secret" value="Your production app secret" />
    <preference name="com.urbanairship.development_app_key" value="P0OVDHQwSDuzAOfrljtjcA" />
    <preference name="com.urbanairship.development_app_secret" value="YYdhwr9KQSepGRxn_-nZMQ" />
    <preference name="com.urbanairship.in_production" value="false" />
    <preference name="com.urbanairship.gcm_sender" value="465394200244" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="PushNotificationPlugin">
        <param name="android-package" value="com.urbanairship.phonegap.PushNotificationPlugin" />
        <param name="onload" value="true" />
    </feature>
</widget>
